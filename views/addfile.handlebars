<h2 class="page-header">Add File</h2>
{{#if errors}}
        <div class="alert alert-danger">{{msg}}</div>
{{/if}}
<form method="post" action="/addfiles">
   <div class="form-group">
    <label>Process</label>
    <input type="text" class="form-control" placeholder="Enter process name" name="process">
  </div>
  <div class="form-group">
    <label>Key</label>
    <textarea class="form-control" placeholder="Enter Key" name="key"></textarea>
  </div>
   <div class="form-group">
    <label>Value</label>
    <textarea class="form-control" placeholder="Enter Value" name="value"></textarea>
  </div>

  <button type="submit" class="btn btn-default">Submit</button>
  <a href="/" type="Cancel" class="btn btn-default">Cancel</a>

</form>